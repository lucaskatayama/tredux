{
  "parser" : "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6" : true,
    "jest": true
  },
  "plugins": [ "import" ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.babel.js"
      }
    }
  },
  "rules": {
    "no-script-url": 0,
    "jsx-a11y/href-no-hash" : 0,
    "jsx-a11y/label-has-for" : 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "import/prefer-default-export": 0
  }
}
